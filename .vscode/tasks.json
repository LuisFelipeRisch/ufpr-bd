{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"./escalona.c",
				"./utils.c",
				"./utils.h",
				"./queue.c",
				"./queue.h",
				"./khan.c",
				"./khan.h",
				"./array.c",
				"./array.h",
				"./conflictSerializable.c",
				"./conflictSerializable.h",
				"-lm",
				"-o",
				"${fileDirname}/bin/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"isDefault": true,
				"kind": "build"
			},
			"detail": "compiler: /usr/bin/gcc"
		}
	]
}